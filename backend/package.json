{
  "name": "agentic-frontier-backend",
  "version": "2.0.0",
  "description": "Real-time visualization backend for Claude Code agent workflows with Supabase integration",
  "main": "server.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test-db": "tsx test-connection.ts",
    "setup-db": "node scripts/setup-database.js",
    "migrate": "supabase migration up",
    "types": "supabase gen types typescript --local > types/supabase.ts"
  },
  "keywords": [
    "claude-code",
    "hooks",
    "real-time",
    "visualization",
    "supabase",
    "sse"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
