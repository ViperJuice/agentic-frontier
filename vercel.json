{
  "version": 2,
  "name": "agentic-frontier",
  "builds": [
    {
      "src": "backend/dist/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/dist/src/app.js"
    }
  ],
  "buildCommand": "cd backend && npm run build",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "backend/dist/src/app.js": {
      "maxDuration": 30
    }
  }
}